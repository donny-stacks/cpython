$ git clone git@github.com:<donny-stacks/cpython.git
$ cd cpython
$ git remote add upstream git@github.com:python/cpython.git
$ git remote -v
origin  git@github.com:<donny-stacks>/cpython.git (fetch)
origin  git@github.com:<donny-stacks>/cpython.git (push)
upstream        git@github.com:python/cpython.git (fetch)
upstream        git@github.com:python/cpython.git (push)
./configure --with-pydebug
